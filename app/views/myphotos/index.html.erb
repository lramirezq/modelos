<h1>Listing myphotos</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Observacion</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @myphotos.each do |myphoto| %>
  <tr>
    <td><%= myphoto.nombre %></td>
    <td><%= myphoto.observacion %></td>
    <td><%= link_to 'Show', myphoto %></td>
    <td><%= link_to 'Edit', edit_myphoto_path(myphoto) %></td>
    <td><%= link_to 'Destroy', myphoto, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Myphoto', new_myphoto_path %>
