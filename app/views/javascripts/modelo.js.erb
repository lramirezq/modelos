<%= params[:idf] %> //competencia_detallecompetencias_attributes_1361005067156_rutmodelo
var tipoid = <%= params[:idf] %>
var tipoid = tipoid.id;
var tipoid = "#"+tipoid;
var nombre = tipoid.replace('rutmodelo', 'nombremodelo');
var apellido = tipoid.replace('rutmodelo', 'apellidomodelo');
var responsable = tipoid.replace('rutmodelo', 'nombreresponsable');
var agencia = tipoid.replace('rutmodelo', 'agenciamadre');

$(nombre).val("<%= @modelo.nombres %>")    
$(apellido).val("<%= @modelo.apellidos %>")  

var nr = "<%=@modelo.nombres_responsable%>";
var ar = "<%=@modelo.apellidos_responsable%>";
var ag = "<%=@modelo.agencia%>";
//Bloquear Responsable cuando corresponda
if (nr=="" && ar==""){
	$(responsable).attr('disabled','disabled');
}else{
	$(responsable).removeAttr('disabled');
	$(responsable).val("");
}
//Bloquear Agencia Cuando Corresponsa
if (ag==""){
	$(agencia).attr('disabled','disabled');
}else{
	$(agencia).removeAttr('disabled');
	$(agencia).val("");
}

$(agencia).val("<%=@modelo.agencia%>");
$(responsable).val("<%= @modelo.nombres_responsable + " " +  @modelo.apellidos_responsable%>")  



if (tipoid == "#modelo_numero_id"){
	var con = confirm("El Modelo \n<%= @modelo.nombres %> <%= @modelo.apellidos %> \nExiste en la Base de datos \nDesea Editar su Ficha?");	
		if(con){
			document.location.href="/modelos/<%= @modelo.id %>/edit";
		}
}